<Window x:Class="GaugesApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GaugesApp"
        
        xmlns:custom="clr-namespace:GaugesApp.CustomControls"

    mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>

        <custom:AnalogClock Grid.Row="0"/>
        <custom:DigitalClock ShowSeconds="False" ColonBlink="False" Grid.Row="1" Margin="0 10 0 0" FontSize="36" HorizontalAlignment="Center"/>
    </Grid>
</Window>
